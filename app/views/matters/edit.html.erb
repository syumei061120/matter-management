<body>
  <div class='matter'>
    <div class='matter-title'>
      <h2>案件情報を編集してください</h2>
    </div>
    <div class="matter-main">
      <%= form_with model: @matter, url: matter_path ,class:"matter-forms", local: true do |f|  %>
      <%= render partial: "shared/matter_form", locals: { f: f } %>
      <div>
        <%= f.submit "案件情報編集",class:"matter-button"%>
      </div>
      <% end %>
    </div>
  </div>
</body>